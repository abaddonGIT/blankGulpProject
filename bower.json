{
  "name": "newProjectBundle",
  "version": "0.0.0",
  "dependencies": {
    "font-awesome": "latest",
    "jquery": "*",
    "fancybox": "^2.1.5",
    "bootstrap-grid-css": "latest",
    "animate.css": "latest",
    "jquery.maskedinput": "~1.4.1",
    "jquery-selectric": "~1.9.5",
    "owl.carousel": "latest",
    "hover": "latest",
    "flexslider": "latest",
    "nouislider": "latest",
    "aos": "latest"
  },
  "overrides": {
    "aos": {
      "src/js/venders/aos": [
        "dist/aos.css",
        "dist/aos.js"
      ]
    },
    "jquery": {
      "src/js/venders/jquery": [
        "dist/jquery.min.js"
      ]
    },
    "fancybox": {
      "src/js/venders/fancybox": [
        "source/jquery.fancybox.pack.js",
        "source/jquery.fancybox.css",
        "source/*.png",
        "source/*.gif"
      ]
    },
    "bootstrap-grid-css": {
      "src/css/grid": [
        "grid.css"
      ]
    },
    "font-awesome": {
      "src/css/font-awesome/css": [
        "css/*"
      ],
      "src/css/font-awesome/fonts": [
        "fonts/*"
      ]
    },
    "animate.css": {
      "src/css/animate": [
        "animate.css",
        "animate.min.css"
      ]
    },
    "wow": {
      "src/js/venders/wow": [
        "dist/wow.js",
        "dist/wow.min.js"
      ]
    },
    "jquery.maskedinput": {
      "src/js/venders/mask": [
        "dist/jquery.maskedinput.min.js"
      ]
    },
    "jquery-selectric": {
      "src/js/venders/selectric": [
        "public/jquery.selectric.min.js",
        "public/selectric.css"
      ]
    },
    "owl.carousel": {
      "src/js/venders/owl": [
        "dist/owl.carousel.min.js",
        "dist/owl.carousel.js",
        "dist/assets/owl.carousel.css",
        "dist/assets/owl.carousel.min.css",
        "dist/assets/ajax-loader.gif",
        "dist/assets/owl.video.play.png"
      ]
    },
    "hover": {
      "src/css/hover": [
        "./css/hover.css",
        "./css/hover-min.css"
      ]
    },
    "flexslider": {
      "src/js/venders/flex": [
        "flexslider.css",
        "jquery.flexslider.js",
        "jquery.flexslider-min.js",
        "images/*",
        "fonts/*"
      ]
    },
    "nouislider": {
      "src/js/venders/nouislider": [
        "./distribute/nouislider.js",
        "./distribute/nouislider.min.css",
        "./distribute/nouislider.min.js"
      ]
    }
  }
}